{
  "presets": [
    ["@babel/preset-env", {
      useBuiltIns: "entry",
      corejs: 3
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      corejs: 3
    }],
    "@babel/plugin-proposal-class-properties",
    ["module-resolver", {"root": ["./src"]}]
  ],
  "env": {
    "production": {
      "plugins": [
        "@babel/plugin-transform-react-constant-elements"
      ]
    }
  }
}
